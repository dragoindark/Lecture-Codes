package task8;

public class task9 {
	   public static void main(String args[]) {
	      Firm VEF = new Firm("VEF", 2);
	      VEF.add(new Employee("Filthy", "Frank", 1100.00)); 
	      VEF.add(new Employee("Lil", "Peep", 1200.00));
		  
	      VEF.add(new Employee("Burak", "Gurel", 1000.00));	  	  	  
	      System.out.println(VEF);
	      VEF.sortSalariesAsc(VEF.getEmployee(),0,2);
	      System.out.println(VEF);
	   }
	}
